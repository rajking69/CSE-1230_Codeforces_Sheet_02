#include<stdio.h>
int main()
{
    int i,A1[100000],temp;
    for(i=0; i<100000; i++)
    {
        scanf("%d",&A1[i]);
    }
    for(i=0; i<100000; i++)
    {
        temp=A1[i];
        if(temp==1999)
        {
            printf("Correct\n");
            break;
        }

        else
            printf("Wrong\n");
    }
    return 0;
}
