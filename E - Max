#include<stdio.h>
int main()
{
    int i,n;
    long long int num[1000],max;
    scanf("%d",&n);
    for(i=0; i<n; i++)
    {
        scanf("%lld",&num[i]);
    }
    max=num[0];
    for(i=0; i<n; i++)
    {
        if(max<num[i])
            max=num[i];
    }
    printf("%lld",max);

    return 0;
}
