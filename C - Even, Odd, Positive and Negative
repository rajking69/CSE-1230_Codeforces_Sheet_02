#include<stdio.h>
int main()

{
    int N,a,i;
    int pos=0,neg=0,ev=0,odd=0;
    scanf("%d",&N);
    for(i=1; i<=N; i++)
    {

        scanf("%d",&a);
        if(a%2==0)
            ev++;
        if(a%2!=0)
            odd++;
        if (a>0)
            pos++;
        if(a<0)
            neg++;
    }

    printf("Even: %d\n",ev);
    printf("Odd: %d\n",odd);
    printf("Positive: %d\n",pos);
    printf("Negative: %d\n",neg);


    return 0;
}
